
<body class="container">
  <div class="col-md-6 col-md-offset-3">
    <h1>Hello <%= name %></h1>
    <button class="btn btn-success" id="show-metrics">
      Bring the metrics
    </button>
    <div id="metrics"></div>
  </div>
</body>

<script>
$('#show-metrics').click((e) => {
  e.preventDefault();
  $.getJSON("/metrics/22", {}, (data) => {
    const content = data.result.map(d => {
      console.log(d)
      return 'timestamp: '+d.timestamp;
    })
    $('#metrics').append(content.join("\n"));
  });
})
</script>